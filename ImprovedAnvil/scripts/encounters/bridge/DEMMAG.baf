// 25th level mage //
// First level Spells (5): Magic Missile x5 //
// Second level Spells (5): Melf Acid Arrow x5 //
// Third level Spells (5): Remove magic x2, V.T x3 //
// Fourth level Spells (5): StoneSkin x3, Firs shield, Minor globe //
// Fifth level Spells (5): spell immunity x2, Breach x3 //
// Sixth level Spells (6): True sight, death spell x3, Chain lightning x2 //
// Seventh level Spells (6): Spell Turning x2, PW stun x3, FoD //
// Eight level Spells (6): Abi dalzim's horrid wilting x6 //
// Ninth level Spells (5): Comet x2, dragons breath x2, dark planetar //
// Scrolls: Pfmw x5, power word kill x2, Ruby Ray x5, spellstrike //

IF
  Allegiance(Myself,ENEMY)
  HPLT(Myself,10)
  Global("Iagrestore","AR0530",0)
THEN
  RESPONSE #100
    SetGlobal("Iagrestore","AR0530",1)
END

IF
  Global("Iagrestore","AR0530",2)
  HasItem("bhaalhp1",Myself)
THEN
  RESPONSE #100
    DestroyItem("bhaalhp1")
END

IF
  Allegiance(Myself,ENEMY)
  OR(2)
   Detect([PC])
   Detect([GOODCUTOFF])
  Global("Iademmag02","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Iademmag02","LOCALS",1)
    ReallyForceSpell(Myself,WIZARD_STONE_SKIN)
    ReallyForceSpell(Myself,WIZARD_SPELL_TURNING)
    ReallyForceSpell(Myself,WIZARD_FIRE_SHIELD_BLUE)
    ReallyForceSpell(Myself,WIZARD_MINOR_GLOBE_OF_INVULNERABILITY)
    ReallyForceSpell(Myself,WIZARD_SUMMON_PLANATAR_EVIL)
    ReallyForceSpell(Myself,WIZARD_SPELL_IMMUNITY_ABJURATION)
END

IF
  Global("Iapcastts","GLOBAL",1)
  Global("Iatsinvulner","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Iatsinvulner","LOCALS",1)
    ApplySpellRES("SPINI70",Myself)
END

IF
  Global("Iapcastts","GLOBAL",0)
