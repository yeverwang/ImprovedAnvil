IF
  InMyArea(Player1)
  OR(7)
    AttackedBy([GOODCUTOFF],DEFAULT)
    AttackedBy([PC],DEFAULT)
    SpellCastOnMe([GOODCUTOFF],0)
    SpellCastOnMe([PC],0)
    HitBy([PC],CRUSHING)
    HitBy([GOODCUTOFF],CRUSHING)
    TookDamage()
  !Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    Enemy()
    Shout(89)
