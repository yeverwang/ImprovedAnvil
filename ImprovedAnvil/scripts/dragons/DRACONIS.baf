IF
  OR(2)
    Allegiance(Myself,ENEMY)
    Range([GOODCUTOFF],75)
  Global("Prepare","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Prepare","LOCALS",1)
    SetGlobalTimer("Iadraconisrest","GLOBAL",2100)
    SetGlobalTimer("absolute","LOCALS",24)
    ReallyForceSpell(Myself,WIZARD_STONE_SKIN)
    ReallyForceSpell(Myself,WIZARD_FIRE_SHIELD_RED)
    ReallyForceSpell(Myself,WIZARD_PROTECTION_FROM_MAGIC_WEAPONS)
    ReallyForceSpell(Myself,WIZARD_PROTECTION_FROM_MAGIC_ENERGY)
    ReallyForceSpellRES("jwglobe",Myself)  // ~Globe of Invulnerability~
    ReallyForceSpellRES("spwii10",Myself)  //
    ReallyForceSpellRES("spwii03",Myself)  //
    ReallyForceSpell(Myself,WIZARD_SPELL_IMMUNITY_ABJURATION)
END

