IF
  OR(2)
    InMyArea(Player1)
    Detect([PC])
  Global("saidpiece","LOCALS",0)
  Global("Iapcastts","GLOBAL",0)
THEN
  RESPONSE #100
    Enemy()
    DisplayStringHead(Myself,70699)  // ~Primes!  Attack!  They may have been forced into service!!~
