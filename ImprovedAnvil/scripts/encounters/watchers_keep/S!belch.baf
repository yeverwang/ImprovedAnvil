// On-death condition //

IF
  Die()
THEN
	RESPONSE #100
    ReallyForceSpellDeadRES("SPINI93",Myself)
END

// Init & pre-buff //

IF
  See(NearestEnemyOf(Myself))
  Global("Iabalor01","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Iabalor01","LOCALS",1)
    SetGlobalTimer("Iabalorrest","GLOBAL",2100)
    ReallyForceSpell(Myself,DRAGON_STONE_SKIN)
    ReallyForceSpell(Myself,WIZARD_IMPROVED_HASTE)
    ReallyForceSpell(Myself,WIZARD_SPELL_IMMUNITY_ABJURATION)
    ReallyForceSpell(Myself,WIZARD_PROTECTION_FROM_MAGIC_WEAPONS) // PfMW
    ReallyForceSpellRES("SPWII02",Myself) //
    ReallyForceSpellRES("SPWII03",Myself) //
    ReallyForceSpellRES("jwglobe",Myself) //
    ReallyForceSpellRES("SPINI94",Myself) // Flaming Body
END

IF
  Allegiance(Myself,ENEMY)
  See(NearestEnemyOf(Myself))
  GlobalTimerExpired("Iabalorrest","GLOBAL")
  Global("Iabalor01","LOCALS",1)
THEN
  RESPONSE #100
    SetGlobalTimer("Iabalorrest","GLOBAL",2100)
    SetGlobal("Iabalor01","LOCALS",0)
    SetGlobal("Iabalor02","LOCALS",0)
    SetGlobal("Iabalor03","LOCALS",0)
    SetGlobal("Iabalor04","LOCALS",0)
    SetGlobal("Iabalor05","LOCALS",0)
    SetGlobal("Iabalor06","LOCALS",0)
    SetGlobal("Iabalor07","LOCALS",0)
    SetGlobal("Iabalor08","LOCALS",0)
    SetGlobal("Iabalor09","LOCALS",0)
    SetGlobal("Iabalor10","LOCALS",0) // unused
    SetGlobal("Iabalor11","LOCALS",0)
    SetGlobal("Iabalor12","LOCALS",0)
    SetGlobal("Ianoward","LOCALS",0)
    SetGlobal("RemoveMagic","LOCALS",0)
    SetGlobal("Iaremove","LOCALS",0)
    SetGlobal("Iaconting","LOCALS",0)
    ReallyForceSpellRES("SPWII07",Myself)
END

// adjusting the difficulty based on SoA / ToB //

IF
  Global("Iabalor01","LOCALS",1)
  Global("Iadifficulty","LOCALS",0)
  GlobalGT("Chapter","GLOBAL",7)
THEN
  RESPONSE #100
    SetGlobal("Iadifficulty","LOCALS",1)
    SetGlobal("IaPFMWAmount","LOCALS",5)
    SetGlobal("IaSkinAmount","LOCALS",5)
    SetGlobal("IaLowerResAmount","LOCALS",5)
    SetGlobal("IaRRoRAmount","LOCALS",5)
END

IF
  Global("Iabalor01","LOCALS",1)
  Global("Iadifficulty","LOCALS",0)
  GlobalLT("Chapter","GLOBAL",8)
THEN
  RESPONSE #100
    SetGlobal("Iadifficulty","LOCALS",1)
    SetGlobal("IaPFMWAmount","LOCALS",2)
    SetGlobal("IaSkinAmount","LOCALS",2)
    SetGlobal("IaLowerResAmount","LOCALS",2)
    SetGlobal("IaRRoRAmount","LOCALS",3)
END

// Contingency: FoD //

IF
  Allegiance(Myself,ENEMY)
  Global("Iapcastts","GLOBAL",0)
  !HPLT(Myself,1)
  See(Player1)
  Class(Player1,MAGE_ALL)
  !CheckStat(Player1,5,SCRIPTINGSTATE7)
  !CheckStatGT(Player1,0,SCRIPTINGSTATE2)
  !StateCheck(Player1,STATE_MIRRORIMAGE)
  CheckStatLT(Player1,51,RESISTMAGIC)
  !CheckStat(Player1,2,SCRIPTINGSTATE3)
  CheckStatGT(Player1,8,SAVEVSDEATH)
  !HasBounceEffects(Player1)
  Global("Iap1nr","GLOBAL",0)
  Global("Iaconting","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Iaconting","LOCALS",1)
    DisplayString(Myself,26328)  //
    ReallyForceSpell(Player1,WIZARD_FINGER_OF_DEATH)
    ReallyForceSpell(Player1,WIZARD_FINGER_OF_DEATH)
    ReallyForceSpell(Player1,WIZARD_FINGER_OF_DEATH)
END

